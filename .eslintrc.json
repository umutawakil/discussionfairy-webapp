{
    "env": {
        "browser": false,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [0],
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}
